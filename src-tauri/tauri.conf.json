{
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "maximized": true,
        "resizable": true,
        "title": "ruscan",
        "width": 800,
        "shadow": true
      }
    ],
    "withGlobalTauri": false
  },
  "build": {
    "beforeBuildCommand": "bun run build",
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "signingIdentity": "-"
    },
    "targets": [
      "nsis"
    ]
  },
  "identifier": "ruscan",
  "plugins": {
    "shell": {
      "open": true
    }
  },
  "productName": "ruscan",
  "version": "1.0.0"
}